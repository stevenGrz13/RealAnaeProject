@model AnaeLogiciel.Models.Projet

@{
    ViewData["Title"] = "Create";
    List<Bailleur> liste = ViewData["listebailleur"] as List<Bailleur>;
    List<Composant> listec = ViewData["listecomposant"] as List<Composant>;
}

<form method="post" action="/Projet/CreateProjet">
<input type="text" name="nom" placeholder="Nom">
<br>
<input type="text" name="details" placeholder="Details">
<br>
<input type="date" name="datedebut" placeholder="Date Debut">
<br>
<input type="date" name="datefin" placeholder="Date Fin">
<br>
<input type="text" name="budget" placeholder="Budget">
<br>
Choisir Bailleur:
<select name="idbailleur" id="idbailleur">
    @foreach (var v in liste)
    {
        <option value="@v.Id">@v.Nom</option>
    }
</select>
<br>
Choisir Composants:
<br>
    @foreach (var v in listec)
    {
        @v.Nom<input type="checkbox" name="idcomposant" value="@v.Id"><br>
    }
<br>
<input type="submit" name="inserer">
</form>

