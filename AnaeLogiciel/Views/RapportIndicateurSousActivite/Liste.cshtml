@{
    ViewBag.Title = "title";
    Layout = "_LayoutGlobalProject";
    List<RapportIndicateurSousActivite> liste = ViewData["listerapportsite"] as List<RapportIndicateurSousActivite>;
    int idoccurencesousactivite = ViewBag.idoccurencesousactivite;
    int idoccurenceactivite = ViewBag.idoccurenceactivite;
}

<h3>LISTE RAPPORT</h3>
<br>

<table class="table">
    <thead>
    <tr>
        <th>Technicien</th>
        <th>Indicateur</th>
        <th>Quantite</th>
        <th>Date</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var v in liste)
    {
        <tr>
            <td>@v.Technicien.Email</td>
            <td>@v.TypeIndicateur.Nom</td>
            <td>@v.Quantite</td>
            <td>@v.DateAction</td>
        </tr>   
    }
    </tbody>
</table>

<form action="/OccurenceSousActivite/RetourVersDetailsOccurenceSousActivite">
    <input type="hidden" name="idoccurenceactivite" value="@idoccurenceactivite">
    <input type="hidden" name="idoccurencesousactivite" value="@idoccurencesousactivite">
    <button type="submit" class="btn btn-primary">Retour</button>
</form>