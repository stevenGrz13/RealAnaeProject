@{
    ViewBag.Title = "title";
    Layout = "_LayoutGlobalProject";
    int idoccurence = ViewBag.idoccurence;
    int idtechnicien = ViewBag.idtechnicien;
    DateOnly dateaction = ViewBag.dateaction;
    int idsourcedeverification = ViewBag.idsourcedeverification;
}

<h2>Rapport / PV</h2>

<br>

<form asp-controller="InsertionSources" asp-action="insertionRapportPV" method="post" enctype="multipart/form-data">
    <input type="hidden" name="idoccurence" value="@idoccurence">
    <input type="hidden" name="idtechnicien" value="@idtechnicien">
    <input type="hidden" name="dateaction" value="@dateaction">
    <input type="hidden" name="idsourcedeverification" value="@idsourcedeverification">
    <div class="form-group">
        <label for="fileUpload">Choisissez un fichier .txt :</label>
        <input type="file" id="fileUpload" name="fileUpload" accept=".txt" />
    </div>
    <div class="form-group">
        <input type="submit" value="Télécharger" class="btn btn-primary" />
    </div>
</form>
